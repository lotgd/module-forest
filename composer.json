{
    "name": "lotgd/module-forest",
    "description": "Adds a forest to the village where players can combat monsters and earn experience.",
    "type": "lotgd-module",
    "license": "AGPL-3.0-or-later",
    "authors": [
        {
            "name": "Basilius Sauter",
            "email": "basilius.sauter@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "LotGD\\Module\\Forest\\": "src/",
            "LotGD\\Module\\Forest\\Tests\\": "tests/"
        }
    },
    "repositories": [
        {"type": "path", "url": "../*"}
    ],
    "require": {
        "lotgd/core": "dev-master as 0.6.0",
        "lotgd/module-village": ">=0.5.0-alpha",
        "lotgd/module-new-day": ">=0.5.0-alpha",
        "lotgd/module-res-fight": "dev-master"
    },
    "require-dev": {
        "phpunit/phpunit": "*",
        "symfony/yaml": "*"
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
