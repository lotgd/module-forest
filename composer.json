{
    "name": "lotgd/module-forest",
    "description": "Adds a forest to the village where players can combat monsters and earn experience.",
    "type": "lotgd-module",
    "license": "AGPL-3.0-or-later",
    "authors": [
        {
            "name": "Basilius Sauter",
            "email": "basilius.sauter@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "LotGD\\Module\\Forest\\": "src/",
            "LotGD\\Module\\Forest\\Tests\\": "tests/"
        }
    },
    "repositories": [
        {
            "type": "path",
            "url": "../core"
        },
        {
            "type": "composer",
            "url": "https://raw.githubusercontent.com/lotgd/packages/master/build/packages.json"
        }
    ],
    "require": {
        "lotgd/core": "dev-feature/better-viewpoint-renderer as 0.6.0-alpha.5",
        "lotgd/module-village": "^0.6",
        "lotgd/module-new-day": "^0.6",
        "lotgd/module-res-fight": "^0.6",
        "lotgd/module-res-wealth": "^0.6"
    },
    "require-dev": {
        "phpunit/phpunit": "*",
        "symfony/yaml": "*"
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
